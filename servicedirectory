# Use OpenJDK as base image
FROM openjdk:17-jdk-slim

# Set working directory
WORKDIR /app/

# Copy JAR file into container
COPY target/ServiceDirectory-0.0.1-SNAPSHOT.jar ServiceDirectory-0.0.1-SNAPSHOT.jar

# Expose application port
EXPOSE 8761

# Command to run the application
ENTRYPOINT ["java", "-jar", "ServiceDirectory-0.0.1-SNAPSHOT.jar"]

